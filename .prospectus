    github_tag
    repo 'jjk-jacky/anopa'
  end

  actual do
    git_tag
    regex /^(.*)-\d+$/
  end

  deps do
    PkgForge.load_from_file.deps.each do |dep_name, current_version|
      item do
        name dep_name

        expected do
          github_release
          repo "amylum/#{dep_name}"
        end

        actual do
          static
          set current_version
        end
      end
    end
  end
